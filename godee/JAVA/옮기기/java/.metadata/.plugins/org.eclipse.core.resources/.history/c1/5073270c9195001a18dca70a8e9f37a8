class Calc {
	
}

public class CalcTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[] num = {10};
		System.out.println(num[0]);
		change(num);
		System.out.println(num[0]);
	}
	
	static void change(int[] num) {
		num[0] = 100;
		System.out.println(num[0]);
	}
}
