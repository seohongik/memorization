class Book {
	String title;
	String genre;
	
	void printBook() {
		System.out.println("力格 : "+  title);
		System.out.println("厘福 : " + genre);
	}
}

class Novel extends Book {
	String writer;
	
	void printNov() {
		printBook();
		System.out.println("历磊 : "+writer);
	}
}

class Magazine extends Book {
	int day;
	
	void printMag() {
		printBook();
		System.out.println("惯概老 : " +day+"老");
	}
}

public class Bookshelf {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
