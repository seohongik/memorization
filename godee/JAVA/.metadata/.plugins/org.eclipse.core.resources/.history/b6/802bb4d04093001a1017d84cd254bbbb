
public class Ex18 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
	int[][] score= {
			{100, 100,100}
			,{20,20,20}
			,{30,30,30}
			,{40,40,40}
			,{50,50,50}			
		};
	
	for(int i=0; i<score.length; i++) {//ÇàºÎºÐ
		for(int j=0; j<score[i].length; j++ ) {
			System.out.printf("%3d",score[i][j]);
		}
		
	}
	
	}

}
