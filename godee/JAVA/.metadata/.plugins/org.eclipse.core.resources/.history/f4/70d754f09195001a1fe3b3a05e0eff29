		// TODO Auto-generated method stub //콜바이 레퍼런스
		//int[]num = {10};
		//System.out.println(num[0]);
		//change(num);
		int num=10;
		System.out.println(num);
		testValue(num);
		//System.out.println(num);
		
		
	}
	static void testValue(int num) {
	
		num=100;	
		System.out.println(num);
	}
	
	
	static void change(int[] num) {
		num[0] =100;
		System.out.println(num[0]);
	}
	
	
