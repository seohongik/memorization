
//package 이름은 dailyTest입니다.
package dailyTest;

//class는 Subject입니다.
public class Subject {

	//필드(멤버 변수)를 정의합니다.
	private String name;	//과목 이름
	private int score; 	//과목 점수
	
		
	//메서드(멤버함수)를 정의합니다.
	
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getScore() {
		return score;
	}
	public void setScore(int score) {
		this.score = score;
	}


	
}
