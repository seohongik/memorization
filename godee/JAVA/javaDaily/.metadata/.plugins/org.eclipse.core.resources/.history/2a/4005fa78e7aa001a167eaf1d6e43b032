import java.util.Scanner;


public class Ex01 {
	public static void main(String[] args) {
		
		Scanner sc= new Scanner(System.in);

			
			
			
			int[] arr= new int[5]; 
			
			
			for(int i=0; i<arr.length; i++) {
				
				String str=sc.nextLine();
				
				char a=str.charAt(0)
				System.out.println(a);
				
			}
			
			
			
			
		
			
			
			
			
			}
		
	
}
