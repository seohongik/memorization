import java.util.Scanner;

public class Ex02 {
	
	
	public static void main(String[] args) {
		
	Scanner sc=new Scanner(System.in);
	
	
	int pure=sc.nextInt();
	
		int j;
		while(pure>=0) {
			
			if(pure==0) {
				
				break;
				
			}
			
			System.out.print(pure+"*");
			j=--pure;
			System.out.println(j*j-1);	
		}
		
		
	
	}
	}

